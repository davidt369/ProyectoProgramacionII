@model IEnumerable<ProyectoProgramacionII.Models.Tbsale>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TotalAmount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UpdatedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkSalestatusNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkUserNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TotalAmount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdatedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkSalestatusNavigation.PkSalestatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkUserNavigation.PkUser)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PkSale">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PkSale">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PkSale">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
